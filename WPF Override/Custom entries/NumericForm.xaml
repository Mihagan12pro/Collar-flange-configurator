<UserControl x:Class="Collar_flange_configurator.WPF_Override.Custom_entries.ElementForm"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Collar_flange_configurator.WPF_Override.Custom_entries"
             xmlns:override="clr-namespace:Collar_flange_configurator.WPF_Override.TextBox"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <StackPanel Orientation="Horizontal" Name="ElementStackPanel">
            <Label FontSize="11" Width="25" Height="25" FontFamily="Calibri" VerticalAlignment="Center" HorizontalContentAlignment="Left" VerticalContentAlignment="Center" Background="#f0f0f0"  Name="ElementLabel"></Label>
        </StackPanel>
    </Grid>
</UserControl>
